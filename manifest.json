{
  "name": "FakeBlock!",
  "description": "Block Twitter posts by keyword",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "storage",
    "webNavigation"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
  	"scripts": ["eventpage.js"],
  	"persistent": false
  },
  "content_scripts": [
    { 
      "matches": ["*://twitter.com/*"],
      "js": ["jquery-2.1.1.js", "fakeblock.js"]
    }
  ]
}



