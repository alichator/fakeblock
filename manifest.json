{
  "name": "FakeBlock!",
  "description": "Block Facebook posts by keyword",
  "manifest_version": 2,
  "version": "1.0",
  "permissions": [
    "webNavigation"
  ],
  "icons": {
  	"16": "Filter2.png",
  	"48": "Filter.png"
  },
  "browser_action": {
    "default_icon": "Filter2.png",
    "default_popup": "popup.html"
  },
  "background": {
  	"scripts": ["eventpage.js"],
  	"persistent": false
  },
  "content_scripts": [
    { 
      "matches": ["*://www.facebook.com/*"],
      "js": ["jquery-2.1.1.js", "fakeblock.js"]
    }
  ]
}



